  "definitions": {
    "IsTenantAvailableInput": {
      "required": [ "tenancyName" ],
      "type": "object",
      "properties": {
        "tenancyName": {
          "maxLength": 64,
          "minLength": 0,
          "type": "string"
        }
      }
    },
    "IsTenantAvailableOutput": {
      "type": "object",
      "properties": {
        "state": {
          "format": "int32",
          "enum": [ 1, 2, 3 ],
          "type": "integer"
        },
        "tenantId": {
          "format": "int32",
          "type": "integer"
        }
      }
    },
    "RegisterInput": {
      "required": [ "name", "surname", "userName", "emailAddress", "password" ],
      "type": "object",
      "properties": {
        "name": {
          "maxLength": 64,
          "minLength": 0,
          "type": "string"
        },
        "surname": {
          "maxLength": 64,
          "minLength": 0,
          "type": "string"
        },
        "userName": {
          "maxLength": 256,
          "minLength": 0,
          "type": "string"
        },
        "emailAddress": {
          "maxLength": 256,
          "minLength": 0,
          "type": "string"
        },
        "password": {
          "maxLength": 32,
          "minLength": 0,
          "type": "string"
        },
        "captchaResponse": { "type": "string" }
      }
    },
    "RegisterOutput": {
      "type": "object",
      "properties": { "canLogin": { "type": "boolean" } }
    },
    "ChangeUiThemeInput": {
      "required": [ "theme" ],
      "type": "object",
      "properties": {
        "theme": {
          "maxLength": 32,
          "minLength": 0,
          "type": "string"
        }
      }
    },
    "PageInput": {
      "type": "object",
      "properties": {
        "page": {
          "format": "int32",
          "type": "integer"
        },
        "pageSize": {
          "format": "int32",
          "type": "integer"
        }
      }
    },
    "CreateRoleDto": {
      "required": [ "name", "displayName" ],
      "type": "object",
      "properties": {
        "name": {
          "maxLength": 32,
          "minLength": 0,
          "type": "string"
        },
        "displayName": {
          "maxLength": 64,
          "minLength": 0,
          "type": "string"
        },
        "normalizedName": { "type": "string" },
        "description": {
